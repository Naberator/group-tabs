{
  "name": "Group Tabs",
  "description": "Group tabs by domain",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "tabs",
    "tabGroups"
  ],
  "action": {
    "default_icon": {
      "16": "/images/icon16.png",
      "32": "/images/icon32.png",
      "48": "/images/icon48.png",
      "128": "/images/icon128.png"
    }
  },
  "icons": {
      "16": "/images/icon16.png",
      "32": "/images/icon32.png",
      "48": "/images/icon48.png",
      "128": "/images/icon128.png"
  },
  "commands": {
    "group-tabs": {
      "suggested_key": {
        "default": "Alt+G",
        "mac": "MacCtrl+G"
      },
      "description": "Groups tabs by domain!"
    }
  }
}
